        <box size="0.02 0.02 0.08" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.05" />
      <geometry>
        <box size="0.02 0.02 0.08" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.05" />
      <mass value="${0.5*10./9.80}" />
      <inertia ixx="0.1" iyy="0.1" izz="0.1" ixy="0" ixz="0" iyz="0" />
    </inertial>
  </link>
  <link name="$(arg robot_name)_link7">
    <visual>
      <origin xyz="0 0 0.05" />
      <geometry>
        <box size="0.02 0.02 0.08" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.05" />
      <geometry>
        <box size="0.02 0.02 0.08" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.05" />
